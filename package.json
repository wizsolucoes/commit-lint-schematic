{
  "name": "@wizsolucoes/commit-lint",
  "version": "1.0.0",
  "description": "Adicione lint de mensagens de commit, segundo a especificação Conventional Commits, ao seu projeto com apenas um comando!",
  "homepage": "https://github.com/wizsolucoes/commit-lint-schematic#wiz-commit-lint-schematic",
  "repository": {
    "type": "git",
    "url": "https://github.com/wizsolucoes/commit-lint-schematic"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^10.0.5",
    "@angular-devkit/schematics": "^10.0.5",
    "@schematics/angular": "^10.1.6",
    "typescript": "~3.9.7"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "jasmine": "^3.5.0"
  }
}
